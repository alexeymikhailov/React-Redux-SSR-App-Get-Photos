    "bootstrap": "^3.3.7",
    "history": "^4.7.2",
    "node-fetch": "^1.7.3",
    "node-noop": "^1.0.0",
    "prop-types": "^15.6.0",
    "prop-types-extra": "^1.0.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "5.0.0-alpha.6",
    "react-transition-group": "^2.2.1",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "stylefmt": "^6.0.0"
