{
  "name": "react-redux-ssr-app-get-photos",
  "version": "1.0.0",
  "description": "Get Photos is the application that shows posts based on specific point or current location",
  "main": "server.js",
  "scripts": {
    "test": "jest",
    "test:update": "jest -- -u",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "dev": "rimraf ./config/static && webpack --config ./config/webpack.dev.conf --progress --profile --colors && cross-env NODE_ENV=development babel-node ./server/index.js",
    "build": "webpack --config ./config/webpack.prod.conf.js --progress --profile --colors && cross-env NODE_ENV=production babel-node ./server/index.js",
    "deploy": "cross-env NODE_ENV=production webpack --progress --colors",
    "flow": "flow check --show-all-errors --color always",
    "flow-typed": "rimraf ./flow-typed/npm && flow-typed install --overwrite",
    "lint": "yarn run lint:js && yarn run lint:css-fix && yarn run lint:css",
    "lint:js": "eslint --ext .js,.jsx src --config .eslintrc --cache --color",
    "lint:css": "stylelint --config .stylelintrc \"src/css/*.css\" --formatter verbose --cache --color",
    "lint:css-fix": "stylefmt --config .stylelintrc --recursive \"src/css/*.css\"",
    "production": "yarn run lint && yarn run deploy"
  },
  "keywords": [
    "react",
    "react-router",
    "react-router-redux",
    "express",
    "webpack",
    "babel",
    "node",
    "server-side-rendering",
    "ssr"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/alexeymikhailov/React-Redux-SSR-App-Get-Photos.git"
  },
  "author": "Alexey M.",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
